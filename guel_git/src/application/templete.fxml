         </children>
      </VBox>
   </top>
   <center>
      <SplitPane fx:id="split" dividerPositions="0.2, 0.7">
        <items>
            <AnchorPane>
               <children>
                  <TreeView fx:id="treeV" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
            <AnchorPane>
               <children>
                  <TabPane fx:id="mainTab" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
