                        <MenuItem fx:id="newWindow" mnemonicParsing="false" text="newWindow" />
                  </items>    
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <ToolBar>
               <items>
                  <Button fx:id="binderbtn" mnemonicParsing="false" prefHeight="23.0" prefWidth="91.0" text="binder" />
                  <Button fx:id="newFilebtn" mnemonicParsing="false" prefHeight="23.0" prefWidth="91.0" text="new file" />
                  <Button fx:id="loadbtn" mnemonicParsing="false" prefHeight="23.0" prefWidth="91.0" text="load Folder" />
                  <Button fx:id="mappingbtn" mnemonicParsing="false" prefHeight="23.0" prefWidth="91.0" text="mapping" />
                  <TextField fx:id="txtMsg" prefHeight="23.0" prefWidth="57.0" />
                  <Button fx:id="btnAlert" mnemonicParsing="false" onAction="#clickHandler" prefHeight="23.0" prefWidth="57.0" text="Alert" />
                  <Region prefHeight="27.0" prefWidth="14.0" />
                  <Label text="text">
                     <font>
                        <Font size="13.0" />
                     </font>
                     <padding>
                        <Insets top="2.0" />
                     </padding>
                  </Label>
                  <!--ChoiceBox accessibleText="글씨체" prefHeight="23.0" prefWidth="80.0"-->
                  <ComboBox prefHeight="23.0" prefWidth="80.0" />
                  <ComboBox prefHeight="23.0" prefWidth="50.0" />
                  <Button mnemonicParsing="false" prefHeight="23.0" prefWidth="20.0" text="Button" />
